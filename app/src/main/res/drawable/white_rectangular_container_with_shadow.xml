<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">

        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#80e4e5e8" />
        </shape>
    </item>

    <!--this is first circle from white rectangle-->
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">

        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#e4e5e8" />
        </shape>
    </item>


    <!--this is white rectangle-->
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/white" />
        </shape>
    </item>

</layer-list>


