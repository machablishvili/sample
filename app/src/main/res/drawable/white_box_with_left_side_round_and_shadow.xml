<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle">
            <corners
                android:bottomLeftRadius="30dp"
                android:topLeftRadius="30dp" />
            <stroke
                android:width="1dp"
                android:color="#80e4e5e8" />
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <corners
                android:bottomLeftRadius="30dp"
                android:topLeftRadius="30dp" />
            <stroke
                android:width="2dp"
                android:color="#e4e5e8" />
        </shape>
    </item>

    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="rectangle">
            <corners
                android:bottomLeftRadius="30dp"
                android:topLeftRadius="30dp" />
            <solid android:color="@color/white" />
        </shape>
    </item>

</layer-list>